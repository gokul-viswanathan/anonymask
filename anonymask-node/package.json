{
  "name": "@anonymask/core",
  "version": "0.3.1",
  "description": "Secure anonymization/de-anonymization library for PII data",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "Gokul Viswanathan <gokulviswanathan25@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/gokul-viswanathan/anonymask.git"
  },
  "license": "MIT",
  "browser": "browser.js",
  "keywords": [
    "anonymization",
    "PII",
    "privacy",
    "LLM",
    "RAG",
    "security",
    "data-protection"
  ],
  "files": [
    "index.d.ts",
    "index.js",
    "browser.js"
  ],
  "napi": {
    "binaryName": "anonymask",
    "targets": [
      "x86_64-pc-windows-msvc",
      "x86_64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "aarch64-unknown-linux-gnu",
      "aarch64-apple-darwin"
    ]
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "bench": "node --import @oxc-node/core/register benchmark/bench.ts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "format": "run-p format:prettier format:rs format:toml",
    "format:prettier": "prettier . -w",
    "format:rs": "cargo fmt",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "jest",
    "preversion": "napi build --platform && git add .",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.0.0",
    "@tybys/wasm-util": "^0.10.0",
    "chalk": "^5.6.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "jest": "^30.2.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "tinybench": "^5.0.1",
    "typescript": "^5.9.2"
  },
  "optionalDependencies": {
    "@anonymask/core-win32-x64-msvc": "0.2.1",
    "@anonymask/core-darwin-x64": "0.2.1",
    "@anonymask/core-linux-x64-gnu": "0.2.1",
    "@anonymask/core-linux-arm64-gnu": "0.2.1",
    "@anonymask/core-darwin-arm64": "0.2.1"
  }
}
